{
	"name": "validate-json-action",
	"version": "1.0.0",
	"description": "Validate JSON(s) by given schema using Ajv schema validator",
	"license": "MIT",
	"author": {
		"name": "Jakub VanÄ›k",
		"email": "info@jakub-vanek.eu"
	},
	"keywords": [
		"json",
		"schema",
		"validator",
		"ajv",
		"action",
		"workflow"
	],
	"main": "src/action.js",
	"scripts": {
		"format": "prettier --write .",
		"lint": "eslint src/**/*.ts",
		"lint:fix": "eslint src/**/*.ts --fix",
		"pretty": "prettier '**/*.{ts,js,json,yaml}' --list-different",
		"pretty:fix": "prettier '**/*.{ts,js,json,yaml}' --write",
		"build": "ncc build src/action.ts -o .dist",
		"start": "node src/action.ts"
	},
	"dependencies": {
		"@actions/core": "^1.9.1",
		"@actions/glob": "^0.2.0",
		"ajv": "^8.12.0",
